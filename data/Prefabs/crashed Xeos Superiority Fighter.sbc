break;
}
if (subMode &gt; 0)
{
sb.Append("Total Display Panels: ").Append(lyts.Count).Append("\n\n");
}
if (subMode &gt; 1)
{
sb.Append("Total Armor Blocks: ").Append(structBlocks.Count).Append('\n');
}
if (subMode &gt; 2)
{
sb.Append("Total Terminal Blocks: ").Append(termBlocks.Count).Append('\n');
}
