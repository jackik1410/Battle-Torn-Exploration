public void PaintCrossBox(char[][] map, int pX1, int pY1, int pX2, int pY2, char code)
{
int mkX = scOffX + (pX1 * (scSzX + 1));
int mkY = scOffY + (pY1 * (scSzY + 1));

int maxX = (pX2 - pX1 + 1) * (scSzX + 1);
int maxY = (pY2 - pY1 + 1) * (scSzY + 1);

