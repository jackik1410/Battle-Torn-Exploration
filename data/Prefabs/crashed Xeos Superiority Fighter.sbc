{
int mkX = scOffX + (pX * (scSzX + 1));
int mkY = scOffY + (pY * (scSzY + 1));

for (int x = 0; x &lt;= scSzX + 1; x++)
{
int y = (scSzX == scSzY ? x : (int)Math.Round(x * boxAspect));
map[mkY + y][mkX + x] = code;
